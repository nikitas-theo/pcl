program yes_and_no;

forward procedure no(x : integer);
forward procedure yes(x : integer);

procedure yes(x : integer);
begin
    if x = 0 then return;
    writeString("YES");
    no(x-1);
end;

procedure no(x : integer);
begin
    if x = 0 then return;
    writeString("NO");
    yes(x-1);
end;

begin
    yes(10);
end.
