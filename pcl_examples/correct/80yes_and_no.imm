define void @main() {
entry:
  call void @GC_init()
  %hidden_struct = alloca %anon
  call void @yes(%anon* %hidden_struct, i32 10)
  call void @writeString(i8* getelementptr inbounds ([2 x i8], [2 x i8]* @str.2, i32 0, i32 0))
  ret void
}

define void @no(%anon* %0, i32 %1) {
entry:
  %x = alloca i32
  store i32 %1, i32* %x
  %hidden_struct = alloca %anon.1
  %2 = getelementptr %anon.1, %anon.1* %hidden_struct, i32 0, i32 0
  store %anon* %0, %anon** %2
  %binop_l = load i32, i32* %x
  %eqtmp = icmp eq i32 %binop_l, 0
  %if_cond = icmp ne i1 %eqtmp, false
  br i1 %if_cond, label %then, label %else

then:                                             ; preds = %entry
  ret void

endif:                                            ; preds = %else
  call void @writeString(i8* getelementptr inbounds ([3 x i8], [3 x i8]* @str.1, i32 0, i32 0))
  %3 = getelementptr %anon.1, %anon.1* %hidden_struct, i32 0, i32 0
  %4 = load %anon*, %anon** %3
  %binop_l1 = load i32, i32* %x
  %addtmp = sub i32 %binop_l1, 1
  call void @yes(%anon* %4, i32 %addtmp)
  ret void

else:                                             ; preds = %entry
  br label %endif
}

define void @yes(%anon* %0, i32 %1) {
entry:
  %x = alloca i32
  store i32 %1, i32* %x
  %hidden_struct = alloca %anon.0
  %2 = getelementptr %anon.0, %anon.0* %hidden_struct, i32 0, i32 0
  store %anon* %0, %anon** %2
  %binop_l = load i32, i32* %x
  %eqtmp = icmp eq i32 %binop_l, 0
  %if_cond = icmp ne i1 %eqtmp, false
  br i1 %if_cond, label %then, label %else

then:                                             ; preds = %entry
  ret void

endif:                                            ; preds = %else
  call void @writeString(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @str, i32 0, i32 0))
  %3 = getelementptr %anon.0, %anon.0* %hidden_struct, i32 0, i32 0
  %4 = load %anon*, %anon** %3
  %binop_l1 = load i32, i32* %x
  %addtmp = sub i32 %binop_l1, 1
  call void @no(%anon* %4, i32 %addtmp)
