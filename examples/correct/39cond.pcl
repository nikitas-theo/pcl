program cond;
   var a         : boolean;
       x, y, sum : integer;
begin
   x   := 10;
   y   := 0;
   a   := true;
   sum := 0;
   
   while a do
      if a and (x>=y) then
      begin
         sum := sum + x * y;
         x := x-1
      end
      else if true then
      begin
         if y<=10 then
            x := 10
         else if not false then
            a := false;
         y := y + 1
      end;
   if sum = 1705 then writeString("42\n") else writeString("wrong!!!\n")
end.
